{
  "name": "@solid-primitives/styles",
  "version": "0.1.1",
  "description": "Collection of reactive primitives focused on styles.",
  "author": "Damian Tarnawski <gthetaranv@gmail.com>",
  "contributors": [
    "Tom Pichaud <dev.tompichaud@icloud.com>"
  ],
  "license": "MIT",
  "homepage": "https://primitives.solidjs.community/package/styles",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solidjs-community/solid-primitives.git"
  },
  "bugs": {
    "url": "https://github.com/solidjs-community/solid-primitives/issues"
  },
  "primitive": {
    "name": "styles",
    "stage": 0,
    "list": [
      "createRemSize"
    ],
    "category": "Display & Media"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "@solid-primitives/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "solid",
    "primitives",
    "styles",
    "css"
  ],
  "dependencies": {
    "@solid-primitives/utils": "^6.3.1",
    "@solid-primitives/rootless": "^1.5.1"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "typesVersions": {},
  "devDependencies": {
    "solid-js": "^1.8.7"
  },
  "scripts": {
    "dev": "tsx ../../scripts/dev.ts",
    "build": "tsx ../../scripts/build.ts",
    "vitest": "vitest -c ../../configs/vitest.config.ts",
    "test": "pnpm run vitest",
    "test:ssr": "pnpm run vitest --mode ssr"
  }
}