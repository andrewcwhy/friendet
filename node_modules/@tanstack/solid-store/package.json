        "default": "./dist/cjs/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@tanstack/store": "0.7.1"
  },
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.10",
    "solid-js": "^1.9.7",
    "vite-plugin-solid": "^2.11.6"
