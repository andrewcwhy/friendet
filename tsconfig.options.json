{
    "compilerOptions": {
        "strict": true,
        "esModuleInterop": true,
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "isolatedModules": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "target": "ESNext",
        "allowJs": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true
    }
}